{
  "name": "sqlite-esm",
  "version": "3.41.2-build15",
  "description": "SQLite Wasm conveniently wrapped as an ES Module.",
  "keywords": [
    "sqlite",
    "sqlite3",
    "sqlite-wasm",
    "sqlite3-wasm",
    "webassembly",
    "wasm",
    "esm",
    "opfs",
    "origin-private-file-system"
  ],
  "main": "index.mjs",
  "type": "module",
  "files": [
    "index.mjs",
    "sqlite-wasm/",
    "sqlite3.wasm"
  ],
  "exports": {
    ".": {
      "import": "./index.mjs",
      "main": "./index.mjs",
      "browser": "./index.mjs"
    },
    "./node": {
      "import": "./sqlite-wasm/jswasm/sqlite3-node.mjs",
      "types": "./index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "sqlite-wasm": "bin/index.js"
  },
  "scripts": {
    "deploy": "npm run prepare && git commit -am 'New release' && git push && npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomayac/sqlite-wasm.git"
  },
  "author": "Thomas Steiner (tomac@google.com)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/tomayac/sqlite-wasm/issues"
  },
  "homepage": "https://github.com/tomayac/sqlite-wasm#readme",
  "devDependencies": {
    "cheerio": "^1.0.0-rc.12",
    "decompress": "^4.2.1",
    "http-server": "github:vapier/http-server",
    "module-workers-polyfill": "^0.3.2",
    "node-fetch": "^3.3.1",
    "prettier": "^2.8.8",
    "publint": "^0.1.11",
    "shx": "^0.3.4"
  }
}
